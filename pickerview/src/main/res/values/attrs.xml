<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="textColorOut" format="color"/>
        <attr name="textColorCenter" format="color"/>
        <attr name="dividerColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="switch_button">
        <attr name="switch_left_text" format="string"/>
        <attr name="switch_right_text" format="string"/>
        <attr name="switch_text_size" format="dimension"/>
        <attr name="select_text_color" format="color"/>
        <attr name="unselect_text_color" format="color"/>
        <attr name="select_text_color_bg" format="color"/>
        <attr name="unselect_text_color_bg" format="color"/>
        <attr name="select_which">
            <enum name="select_left" value="0"/>
            <enum name="select_right" value="1"/>
        </attr>
    </declare-styleable>
</resources>